<?xml version="1.0" ?>
<object-set
   xmlns="http://www.crt.realtors.org/xml/variman/object-set/1"
   resource-key="abc123">
  <object-group type="Photo">
    <object src="abc123-1.jpg"
            description="Beautiful frontal view of home." />
    <object default="true" src="abc123-2.jpg" description="">
    </object>
    <object src="abc123-1.gif" />
  </object-group>

  <object-group type="Documents" base="../dirTest/">
    <object src="foo.xml"
            description="Some basic document." />
    <object src="2002 tax records.rtf"
            description="County tax records for 2002." />
    <object default="true" src="a/bar.html"
            description="Disclosure statement from current owners." />
  </object-group>

  <object-group type="Video" base="http://example.com/video/">
    <object src="abc123-tour.mov"
            description="Take a virtual tour of this house." />
  </object-group>
</object-set>
